model GamePlaySessionExpansion {
  id String @id @default(cuid(2))

  session   GamePlaySession @relation(fields: [sessionId], references: [id], onDelete: Cascade)
  sessionId String          @map("session_id")

  expansion   GameExpansion @relation(fields: [expansionId], references: [id], onDelete: Cascade)
  expansionId String        @map("expansion_id")

  @@unique([sessionId, expansionId])
  @@map("game_play_session_expansions")
}
